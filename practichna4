def X():
    for i in range(7):
        row = ""
        for j in range(5):
            if i == 0 or i == 1 or i == 5 or i == 6:
                if j == 0 or j == 4:
                    row = row + "*"
            if i == 0 or i == 1 or i == 5 or i == 6:
                if j == 1 or j == 2 or j == 3:
                    row = row + " "
            if i == 2 or i == 4:
                if j == 0 or j == 2:
                    row = row + " "
            if i == 2 or i == 4:
                if j == 1 or j == 3:
                    row = row + "*"
            if i == 3:
                if j == 0 or j == 1:
                    row = row + " "
            if i == 3:
                if j == 3:
                    row = row + "*"
        print(row)
def Y():
    for i in range(7):
        row = ""
        for j in range(5):
            if i == 0 or i == 1:
                if j == 0 or j == 4:
                    row = row + "*"
            if i == 0 or i == 1:
                if j == 1 or j == 2 or j == 3:
                    row = row + " "
            if i == 2 or i == 3 or i == 4 or i == 5 or i == 6:
                if j == 0:
                    row = row + " "
            if i == 2:
                if j == 1 or j == 3:
                    row = row + "*"
            if i == 2:
                if j == 2:
                    row = row + " "
            if i == 3 or i == 4 or i == 5 or i == 6:
                if j == 1:
                    row = row + " "
            if i == 3 or i == 4 or i == 5 or i == 6:
                if j == 2:
                    row = row + "*"
        print(row)
def Z():
    for i in range(7):
        row = ""
        for j in range(5):
            if i == 0 or i == 6:
                if j == 0 or j == 1 or j == 2 or j == 3 or j == 4:
                    row = row + "*"
            if i == 1 or i == 2 or i == 3 or i == 4 or i == 5:
                if j == 4:
                    row = row + "*"
            if i == 1:
                if j == 0 or j == 1 or j == 2 or j == 3 or j == 4:
                    row = row + " "
            if i == 2:
                if j == 0 or j == 1 or j == 2:
                    row = row + " "
            if i == 3:
                if j == 0 or j == 1:
                    row = row + " "
            if i == 4:
                if j == 0:
                    row = row + " "
        print(row)
def B():
    for i in range(7):
        row = ""
        for j in range(5):
            if i == 0 or i == 1 or i == 2 or i == 3 or i == 4 or i == 5 or i == 6:
                if j == 0:
                    row = row + "*"
            if i == 0 or i == 3 or i == 6:
                if j == 1 or j == 2 or j == 3:
                    row = row + "*"
            if i == 1 or i == 2 or i == 4 or i == 5:
                if j == 1 or j == 2 or j == 3:
                    row = row + " "
            if i == 1 or i == 2 or i == 4 or i == 5:
                if j == 4:
                    row = row + "*"
        print(row)
def A():
    for i in range(7):
        row = ""
        for j in range(5):
            if i == 0:
                if j == 0:
                    row = row + " "
            if i == 1 or i == 2 or i == 3 or i == 4 or i == 5 or i == 6:
                if j == 0 or j == 4:
                    row = row + "*"
            if i == 1 or i == 2 or i == 4 or i == 5 or i == 6:
                if j == 1 or j == 2 or j == 3:
                    row = row + " "
            if i == 0 or i == 3:
                if j == 1 or j == 2 or j == 3:
                    row = row + "*"
        print(row)
    
nazar = input("Введіть імя з великої на англійській:").upper()
for place in nazar:
    if "A" in place:
        A()
        print("\n")
    if "B" in place:
        B()
        print("\n")
