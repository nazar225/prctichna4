def G():
    for i in range(7):
        row = ""
        for j in range(5):
            if i == 1 or i == 2 or i == 3 or i == 4 or i == 5:
                if j == 0:
                    row = row + "*"
            if i == 0 or i == 6:
                if j == 0:
                    row = row + " "
            if i == 0 or i == 6:
                if j == 1 or j == 2:
                    row == row + "*"
            if i == 1 or i == 2 or i == 4 or i == 5:
                if j == 1 or j == 2 or j == 3:
                    row = row + " "
            if i == 3:
                if j == 1 or j == 2:
                    row = row  + " "
            if i == 0 or i == 6:
                if j == 1 or j == 2:
                    row = row + "*"
            if i == 0 or i == 3 or i == 6:
                if j == 3:
                    row = row + "*"
            if i == 1 or i == 3 or i == 4 or i == 5:
                if j == 4:
                    row = row + "*"
        print(row)
